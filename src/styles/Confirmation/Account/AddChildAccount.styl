.AddChildAccounts
  .main
    width 1200px
    margin 20px auto
    background #fff
    min-height 1200px
    position relative
    .container
      .content
        .content-body
          width 100%
          float left
          .agreement-content
            width 100%;
            position relative
            text-align center
          .buttons
            margin-top 60px
            a
             display inline-block
             width 120px
             height 40px
             text-align center
             line-height 40px
             font-size 18px
             border-radius 5px
          .quit
             background-color #999
             color #fff
          .submit
            background-color #22a7ea
            color #fff
            margin-left 30px
          .title
            height 46px
            margin  20px 0 0 15px
            line-height 46px
            padding-left 40px
            color #fff
            font-size 20px;
          //background url("../../../../static/images/common/title.png") no-repeat
          .child-template
            width 1100px
            position relative
            clear both
            margin-top 20px
            margin-left auto
            margin-right auto
            background-color #fafafa
            height auto
            padding-top 1px
          .child-template-list
            width 1060px
            margin 20px auto
            background-color #fff
            height auto
          .list-item
            padding 20px
          .fill-background
            clear both
            width 100%
            position relative
            height 20px
            background-color #fafafa
          .new-child-account
            position relative
            left 210px
            top -20px
            font-size 16px
            color #22a7ea
          .border-bottom
            width 100%
            height 1px
            border-bottom 1px solid #ddd
            margin-top: 20px
          .account-fill
            position relative
            clear both
            margin-top 20px
            width 1100px
            margin-left auto
            margin-right auto
            background-color #fafafa
            height 220px
            padding-top 1px
            .show-info-list
              position relative
              width 1060px
              margin 20px
              background-color #fff
              height 160px
              padding-top 20px
              .info-list
                width 500px
                float left
                margin-bottom 40px
              .empty-template
                text-align center
                margin-top 60px
                p.tips
                  color #999
        .distribution-template
          margin-top 30px



