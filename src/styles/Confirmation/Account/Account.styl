.Accounts
  .main
    width 1200px
    margin 20px auto
    background #fff
    min-height 1700px
    position relative
    .container
      .tap
        width 630px
        margin 0 auto 40px auto
        padding-top 30px
        div
          width 300px
          cursor pointer
          -webkit-box-shadow  none
          box-shadow none
          line-height 48px
          text-align center
          display: inline-block
          vertical-align: bottom
        .btn-active
          background #22a7ea
          color #fff
          height 48px
        .btn-default
          background #fff
          color #22a7ea
          border 1px solid #22a7ea
          height 46px
      .content
        .content-body
          width 600px
          float left
          .title
            height 46px
            margin  20px 0 0 15px
            line-height 46px
            padding-left 40px
            color #fff
            font-size 20px;
          //background url("../../../../static/images/common/title.png") no-repeat
          .left-side
            margin-left 30px
          .common-style
            margin-top 30px
            width 570px
            background #fafafa
            height 300px
            position relative
            .card
              position relative
              display inline-block
              width 520px
              height 260px
              background-color #fff
            a.changePassword
              display block
              position absolute
              color #22a7ea
              right 100px
              top 40px
              font-family "微软雅黑 Light"
            .real-name-state
              position absolute
              right 80px
              top 160px
            .line
              height 30px
              padding-left 20px
              clear both
              span
              font-size 16px
              color #666
              b
                font-size 16px
                color #666
                font-weight normal
            .right-card-content
              margin 50px 0 0 175px
              .right-line
                position relative
                height 30px
                span
                  font-size 16px
                  color #666
                b
                  font-size 16px
                  color #666
                  font-weight normal
      .sign-management
        clear both
        padding-top 20px
        .title
          height 46px
          margin 20px 0 0 15px
          line-height 46px
          padding-left 40px
          color #fff
          font-size 20px
        .sign-content
          margin 50px auto 0
          .sign-picture
            width 220px
            height 220px
            border 2px dotted #22a7ea
            float left
            margin-left 350px
            margin-right 40px
            img
              width 110px
              height 110px
              margin auto
          .create-seal
            float left
            width 220px
            height 220px
            span
              position relative
              display block
              text-align center
              font-size 14px
              top 20px
              color #22a7ea
            b.tips
              position relative
              display block
              top 45px
              color red
              font-size 10px
              text-align center
            input
              position relative
              display block
              top 60px
              left 12px
              width 196px
              height 40px
              border none
              border-bottom 1px solid #ddd
              padding-left 10px
            a
              position relative
              display block
              top 75px
              left 12px
              width 194px
              height 40px
              border 1px solid #22a7ea
              border-radius 4px
              color #22a7ea
              line-height 40px
              text-align center
    .seal-management
      clear both
      padding-top 60px
      .title
        height 46px
        margin 20px 0 0 15px
        line-height 46px
        padding-left 40px
        color #fff
        font-size 20px
      .child-account
        .account-list
          width 1080px
          margin-left 60px
          margin-top 20px
          clear both
          .list-content
            position relative
            float left
            width 355px
            height:250px
            .operate
              position absolute
              top 110px
              right 20px
              width 90px
              a
                width 80px
                text-align center
                line-height 40px
                display inline-block
                border-radius 10px
                margin-bottom 10px
              a.frozen,a.management, a.finish
                color #22a7ea
                border 1px solid #22a7ea
                height 36px
              a.thaw , a.edit
                color #fff
                background-color  #22a7ea
                height 38px
            ul
              margin 50px 20px 0 40px
              li
                height 35px
                span
                  font-size 16px
                  color #666
                b
                  font-weight normal
                  font-size 16px
                  color #666
                  padding-left 20px
                b.state
                  color #ff8a00
                  font-weight bold
                .template-number
                  color #22a7ea
                  font-size 20px
                  font-weight bold
          .left
            float left
            width 355px
            height:355px
            text-align center
            cursor pointer
            i
              margin-top 70px
              font-size 80px
              color #999
      .right
        float left
        margin-left 110px
        margin-top 40px
      .question
        height auto
        a.serial-number
          display inline-block
          font-family italic
          width 40px
          height 40px
          text-align center
          font-weight bolder
          border-radius 4px
          b
            font-size 20px
            color #666
            line-height 40px
        p.question-title
          position relative
          top -40px
          left 70px
          font-size 18px
        span.question-answer
          color #666
          position relative
          top -30px
          left 70px
          font-size 14px





